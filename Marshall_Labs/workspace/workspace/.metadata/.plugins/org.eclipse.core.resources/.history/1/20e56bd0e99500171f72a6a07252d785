#ifndef MODULE
#define MODULE
#endif

#ifndef __KERNEL__
#define __KERNEL__
#endif

#include <linux/module.h>
#include <linux/kernel.h>
#include <asm/io.h>

MODULE_LICENSE("GPL");


int init_module(void){
	unsigned long* ptr;
	//unsigned long* gp;


	ptr = (unsigned long*)ioremap(0x3F20001c, 0x32);
	*ptr |=0x04;

	printk("MODULE INSTALLED");
	return(0);
}

void cleanup_module(void){

	printk("MODULE REMOVED");

}
