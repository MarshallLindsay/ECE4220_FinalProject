#ifndef MODULE
#define MODULE
#endif

#ifndef __KERNEL__
#define __KERNEL__
#endif

#include <linux/module.h>
#include <linux/kernel.h>
#include <linux/delay.h>
#include <asm/io.h>

MODULE_LICENSE("GPL");

//4009000000

int init_module(void){
	unsigned long* GPFSEL;
	unsigned long* GPSET;
	unsigned long* GPCLR;
	unsigned long* GPLEV;

	int x;

	GPFSEL = (unsigned long*)ioremap(0x3F200000, 0x1c);

	GPSET = (unsigned long*)ioremap(0x3F20001c, 0x4);

	GPCLR = (unsigned long*)ioremap(0x3F200028, 0x4);

	GPLEV = (unsigned long*)ioremap(0x7E200034, 0x04);

	*GPFSEL |= 0x4009000000;

	x = *(GPLEV);
	printk("\n%d", x);
	mswait(5000);
	x = *(GPLEV);
	printk("\n%d", x);




	return(0);
}

void cleanup_module(void){

	printk("MODULE REMOVED");

}
